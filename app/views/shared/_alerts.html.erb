<% if flash[:alert] %>
<%= js add_gritter(flash[:alert], title: "Overtime app Alert", sticky: false) %>
<% elsif flash[:error] %>
<%= js add_gritter(flash[:error], title: "Overtime app Error", sticky: false) %>
<% else %>
<%= js add_gritter(flash[:notice], title: "Overtime app Notice", sticky: false) %>
<% end %>
